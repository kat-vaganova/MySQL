caterina@caterina-EP41-UD3L:~$ redis-cli

-- просмотр адресов
127.0.0.1:6379> SMEMBERS ip
(empty list or set)

 -- подсчет адресов
127.0.0.1:6379> SCARD ip
(integer) 0

 -- создание адресов
127.0.0.1:6379> SADD ip '127.0.0.1'
(integer) 1

127.0.0.1:6379> SCARD ip
(integer) 1
127.0.0.1:6379> SMEMBERS ip
1) "127.0.0.1"
127.0.0.1:6379> SADD ip '127.0.0.2'
(integer) 1
127.0.0.1:6379> SMEMBERS ip
1) "127.0.0.1"
2) "127.0.0.2"
127.0.0.1:6379> SCARD ip
(integer) 2
127.0.0.1:6379> 


